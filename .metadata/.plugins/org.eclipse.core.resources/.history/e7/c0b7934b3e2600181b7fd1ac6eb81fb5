package javaPracticeDemo;

import java.io.File;

public class CreateFileClass {

	public static void main(String[] args) {
		
		File f = new File("Ritesh.txt");
		System.out.println(f.exists());
		
		try {
			f.createNewFile();
			System.out.println(f.exists());
			}catch(Exception e) {
			System.out.println("File doesn't exist" + e);
		}
		

	}

}
